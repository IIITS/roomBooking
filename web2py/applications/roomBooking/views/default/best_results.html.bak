              <a href={{=URL('confirm', args=(row.room_no,start_time,stop_time,subject,a,b,c,d,flag1,sub))}}> <button style="margin-left:150px"> BOOK </button></a>
              </strong></h1></caption>
          
           <tbody style="font-size:20px">
           <tr>
               <td class="danger">
                  Room Name:
              </td>
            <!--   
               <td class="danger">
                  seats:
              </td>
            -->
              <td class = "danger">
                Slot Starts :
              </td  >
            <td class = "danger">
                Slot Ends:
            </td>
                
