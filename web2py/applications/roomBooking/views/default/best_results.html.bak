                {{=row.seats}}
              </td>-->
              <td class = "warning">
                {{=start_time}}
              </td  >
            <td class = "warning">
                {{=stop_time}}
            </td>
                
                <td class="warning">
                {{=row.projector}}
              </td>
              <td class = "warning">
                {{=row.Air_Conditioned}}
              </td  >
            <td class = "warning">
                {{=row.computers}}
            </td>
                
                
            </tr>
            
          </tbody>
          </table>
           
           {{ if count==0:}}
              {{ if flag1=="True": }}
         <h2>          
             <br>
                       Related Searches:
            </h2>
         
            <hr>
            
            
            {{pass}}
            {{pass}}
            
        {{ count+=1 }}
